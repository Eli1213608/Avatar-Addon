{
	"format_version": "1.13.0",
	"minecraft:entity": {
		"description": {
			"identifier": "a:metal_shot",
			"is_spawnable": false,
			"is_summonable": true,
			"is_experimental": false
		},
		"component_groups": {
			"minecraft:exploding": {
				"minecraft:explode": {
					"fuse_length": 0,
					"fuse_lit": true,
					"power": 1,
					"causes_fire": false,
					"fire_affected_by_griefing": true,
					"destroy_affected_by_griefing": true
				}
			},
			"minecraft:despawn": {
				"minecraft:instant_despawn": {}
			}
		},
		"components": {
			"minecraft:physics": {
				"has_gravity": false
			},
			"minecraft:collision_box": {
				"width": 0.31,
				"height": 0.31
			},
			"minecraft:environment_sensor": {
				"triggers": [
					{
						"event": "minecraft:despawn",
						"filters": {
							"test": "distance_to_nearest_player",
							"value": 164,
							"operator": ">"
						},
						"target": "self"
					}
				]
			},
			"minecraft:projectile": {
				"on_hit": {
					"definition_event": {
						"affect_projectile": true,
						"event_trigger": {
							"event": "minecraft:explode",
							"target": "self"
						}
					}
				},
				"power": 1,
				"gravity": 0.007,
				"uncertainty_base": 0,
				"uncertainty_multiplier": 0,
				"anchor": 1,
				"offset": [
					0,
					0,
					0
				],
				"reflect_on_hurt": false,
				"catch_fire": false
			},
			"minecraft:conditional_bandwidth_optimization": {
				"default_values": {
					"max_optimized_distance": 80,
					"max_dropped_ticks": 10,
					"use_motion_prediction_hints": true
				}
			},
			"minecraft:damage_sensor": {
				"triggers": [
					{
						"deals_damage": false,
						"on_damage": {
							"event": "minecraft:despawn"
						},
						"cause": "all"
					}
				]
			},
			"minecraft:despawn": {
				"despawn_from_distance": {
					"max_distance": 50
				}
			}
		},
		"events": {
			"minecraft:explode": {
				"add": {
					"component_groups": [
						"minecraft:exploding"
					]
				}
			},
			"minecraft:despawn": {
				"add": {
					"component_groups": [
						"minecraft:despawn"
					]
				}
			}
		}
	}
}